<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.landlordproperty">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Property Management"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddTenantDetails"></activity>
        <activity android:name=".AddDetailsFlat" />
        <activity android:name=".AddFlats" />
        <activity android:name=".AppartmentDetails" />
        <activity android:name=".AddTenant" />
        <activity android:name=".Payment" />
        <activity android:name=".AddAppartment" />
        <activity android:name=".Appartments" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>